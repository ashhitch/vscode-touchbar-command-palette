{
  "name": "touchbar-command-palette",
  "displayName": "Touchbar Commands",
  "description": "Launch Command Palete, Terminal, Commenting Lines or Quick Open from touchbar",
  "version": "0.0.2",
  "publisher": "ashhitch",
  "engines": {
    "vscode": "^1.17.0"
  },
  "homepage": "https://github.com/ashhitch/vscode-touchbar-command-palette",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashhitch/vscode-touchbar-command-palette.git"
  },
  "bugs": "https://github.com/ashhitch/vscode-touchbar-command-palette/issues",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.touchbarCommandPalette",
    "onCommand:extension.touchbarQuickOpen",
    "onCommand:extension.touchbarTerminal",
    "onCommand:extension.touchbarComment"
  ],
  "main": "./out/extension",
  "icon": "icon.png",
  "contributes": {
    "commands": [
      {
        "command": "extension.touchbarCommandPalette",
        "title": "CMD P"
      },
      {
        "command": "extension.touchbarQuickOpen",
        "title": "Q Open"
      },
      {
        "command": "extension.touchbarTerminal",
        "title": "Term"
      },
      {
        "command": "extension.touchbarComment",
        "title": "// "
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "extension.touchbarCommandPalette",
          "group": "cmd"
        },
        {
          "command": "extension.touchbarQuickOpen",
          "group": "cmd"
        },
        {
          "command": "extension.touchbarTerminal",
          "group": "cmd"
        },
        {
          "command": "extension.touchbarComment",
          "group": "cmd"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.3",
    "vscode": "^1.1.5",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  }
}
